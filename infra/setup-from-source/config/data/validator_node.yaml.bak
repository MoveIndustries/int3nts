base:
  data_dir: UPDATE_TO_YOUR_PATH/data/maptos
  role: validator
  waypoint:
    from_file: UPDATE_TO_YOUR_PATH/data/waypoint.txt
consensus:
  vote_back_pressure_limit: 50
  safety_rules:
    service:
      type: local
    backend:
      type: on_disk_storage
      path: UPDATE_TO_YOUR_PATH/data/secure-data.json
      namespace: null
    initial_safety_rules_config:
      from_file:
        waypoint:
          from_file: UPDATE_TO_YOUR_PATH/data/waypoint.txt
        identity_blob_path: UPDATE_TO_YOUR_PATH/validator-identity.yaml

execution:
  genesis_file_location: UPDATE_TO_YOUR_PATH/data/genesis.blob
storage:
  backup_service_address: 0.0.0.0:6186
  rocksdb_configs:
    enable_storage_sharding: true
validator_network:
  discovery_method: none
  mutual_authentication: true
  identity:
    type: from_config
    key: "0xf8f363b0fd4d8e0e3174cf5f115330142aa9bf96768cfe69924006e9d3fe5d65"
    peer_id: "8b53b97417e598d94209575487ab64bd16ca3e1e965f38f82e50a3a0fcf16e43"
  listen_address: /ip4/0.0.0.0/tcp/6180
api:
  enabled: true
  address: 0.0.0.0:8080
admin_service:
  enabled: true
  address: 0.0.0.0
  port: 9102
state_sync:
  state_sync_driver:
    bootstrapping_mode: ExecuteOrApplyFromGenesis
    continuous_syncing_mode: ExecuteTransactionsOrApplyOutputs
    enable_auto_bootstrapping: true
    max_connection_deadline_secs: 1

